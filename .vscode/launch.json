    "version": "0.2.0",
    "configurations": [
        {
            "name": "Yarn build",
            "request": "launch",
            "runtimeArgs": [
                "build",
            ],
            "runtimeExecutable": "yarn",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
        {
            "name": "Yarn eslint",
            "request": "launch",
            "runtimeArgs": [
                "eslint",
                "lib/eleventy-jsx-template/index.js"
            ],
            "runtimeExecutable": "yarn",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
    ]
}